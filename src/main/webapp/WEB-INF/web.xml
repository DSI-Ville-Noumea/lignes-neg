<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app id="WebApp_ID">
	<display-name>lignesNegatives</display-name>

      <context-param>
         <param-name>HOST_SGBD</param-name>
         <param-value>noumea</param-value>
		<description></description>
      </context-param>
      <context-param>
         <param-name>HOST_SGBD_ADMIN</param-name>
         <param-value>***REMOVED***</param-value>
<description></description>
      </context-param>
      <context-param>
         <param-name>HOST_SGBD_PWD</param-name>
         <param-value>***REMOVED***</param-value>
<description></description>
      </context-param>
 
	<servlet>
		<servlet-name>LignesNegativesServlet</servlet-name>
		<display-name>LignesNegativesServlet</display-name>
		<description></description>
		<servlet-class>
		nc.mairie.lignesnegatives.servlet.LignesNegativesServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>LignesNegativesServlet</servlet-name>
		<url-pattern>/LignesNegativesServlet</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>LignesNegatives.html</welcome-file>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>


	


</web-app>
